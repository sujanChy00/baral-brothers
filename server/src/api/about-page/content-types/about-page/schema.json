{
  "kind": "singleType",
  "collectionName": "about_pages",
  "info": {
    "singularName": "about-page",
    "pluralName": "about-pages",
    "displayName": "About Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "banner": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "text",
      "default": "Baral & Brothers",
      "required": true
    },
    "faq": {
      "type": "component",
      "repeatable": true,
      "component": "faq.faq"
    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "social-links.social-links"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "page-seo.seo"
    }
  }
}
